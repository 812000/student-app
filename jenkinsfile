pipeline {
  agent any
  stages {
    stage('Clone') {
      steps {
        git 'https://github.com/812000/student-app.git'
      }
    }
    stage('Build Image') {
      steps {
        sh 'docker build -t priya812000/student-app .'
      }
    }
    stage('Push Image') {
      steps {
          sh 'docker push <dockerhub-username>/student-app'
        }
      }
    }
  }
